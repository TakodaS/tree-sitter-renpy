    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "header"
          },
          {
            "type": "SYMBOL",
            "name": "statement"
          },
          {
            "type": "SYMBOL",
            "name": "python_block"
          },
          {
            "type": "SYMBOL",
