{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "renpy",
      "camelcase": "Renpy",
      "title": "Renpy",
      "scope": "source.renpy",
      "file-types": [
        "rpy"
      ],
      "injection-regex": "^renpy$",
      "class-name": "TreeSitterRenpy"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter parser for the Renpy language",
    "authors": [
      {
        "name": "Austen Bolitho",
        "email": "austen.bolitho@protonmail.com",
        "url": "https://github.com/TakodaS"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-renpy"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}